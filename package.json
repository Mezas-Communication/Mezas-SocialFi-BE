{
  "name": "mezas-be",
  "version": "1.0.0",
  "description": "",
  "main": "build/src/server.js",
  "scripts": {
    "build": "tsoa spec-and-routes && tsc",
    "start": "node build/src/server.js",
    "format": "yarn prettier --write .",
    "dev": "docker-compose -f ./docker-compose.local.yml up -d --build && concurrently \"nodemon\"  \"nodemon -x tsoa spec-and-routes\"",
    "db:migration": "source .env && sequelize-auto -o \"./src/schemas\" -d $DB_NAME -h $DB_HOST -u $DB_USER -p $DB_PORT -x $DB_PASSWORD -e postgres -l ts",
    "postinstall": "patch-package && husky install",
    "test": "jest --coverage --detectOpenHandles --verbose",
    "lint": "eslint --ext .ts src"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1594.0",
    "axios": "^0.27.2",
    "bignumber.js": "^9.1.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.2",
    "ethers": "5.4.5",
    "express": "4.18.1",
    "file-type": "16.5.4",
    "google-auth-library": "^8.9.0",
    "http": "^0.0.1-security",
    "ipfs-http-client": "56.0.3",
    "is-svg": "4.4.0",
    "json-bigint": "^1.0.0",
    "jsonwebtoken": "^9.0.0",
    "mime-types": "^2.1.35",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "multer": "1.4.2",
    "node-cron": "^3.0.2",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "redis": "4.6.8",
    "response-time": "^2.3.2",
    "sequelize": "^6.31.1",
    "swagger-ui-express": "^4.5.0",
    "tsoa": "4.1.3",
    "uuid": "^9.0.1",
    "web3": "^1.7.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.2",
    "@types/json-bigint": "^1.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.17",
    "@types/node-cron": "^3.0.4",
    "@types/response-time": "^2.3.5",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.19",
    "patch-package": "^6.5.0",
