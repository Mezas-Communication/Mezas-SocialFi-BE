  "_moduleAliases": {
    "@app": "./build/src/app/index.js",
    "@schemas": "./build/src/schemas/index.js",
    "@constants": "./build/src/constants/index.js",
    "@middlewares": "./build/src/middlewares/index.js",
    "@providers": "./build/src/providers/index.js"
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint",
      "yarn format",
      "git add ."
    ]
  }
}
